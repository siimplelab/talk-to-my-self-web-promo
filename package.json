{
  "name": "ttms-promo",
  "version": "1.0.0",
  "description": "ttms-promo",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass --watch style.scss -o style.css",
    "run-jekyll": "bundle exec jekyll serve --watch",
    "run-jekyll-build": "bundle exec jekyll build --watch",
    "run-bs": "browser-sync start --config bs-config.js",
    "run-sass": "sass --watch ./src/scss/style.scss:./src/css/style.css",
    "run-autoprefixer": "npx postcss ./src/css/style.css --use autoprefixer -d ./src/css/",
    "serve": "npx npm-run-all -p run-jekyll run-bs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://exsp-hyungsub08@bitbucket.org/exsp-web-kr/exsp-web-kr.bitbucket.io.git"
